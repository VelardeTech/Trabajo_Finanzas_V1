@model Trabajo_Finanzas_V1.Models.Cliente

<h1>Registro</h1>

<form asp-action="Register">
    <input type="hidden" asp-for="IdUser" />
    <div class="form-group">
        <label asp-for="Nombre">Nombre</label>
        <input asp-for="Nombre" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Apellido">Apellido</label>
        <input asp-for="Apellido" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Dni">DNI</label>
        <input asp-for="Dni" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Telefono">Teléfono</label>
        <input asp-for="Telefono" class="form-control"/>
    </div>
    <div class="form-group">
        <label asp-for="CorreoElectronico">Correo electrónico</label>
        <input asp-for="CorreoElectronico" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Contrasena">Contraseña</label>
        <input asp-for="Contrasena" class="form-control" />
    </div>
    <br />
    <button type="submit" class="btn btn-primary">Registrarse</button>
</form>
